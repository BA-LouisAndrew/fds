{
  "name": "fds",
  "version": "1.0.0",
  "description": "Fraud Detection Service backend",
  "main": "index.js",
  "dependencies": {
    "@prisma/client": "3.14.0",
    "amqplib": "0.10.0",
    "body-parser": "1.20.0",
    "cors": "2.8.5",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "express": "4.18.1",
    "express-winston": "4.2.0",
    "got": "11.8.5",
    "jsonpath": "1.1.1",
    "redis": "4.1.0",
    "swagger-ui-express": "4.4.0",
    "tiny-emitter": "2.1.0",
    "tsoa": "4.0.0",
    "uuid": "8.3.2",
    "wait-port": "0.2.9",
    "winston": "3.8.1"
  },
  "devDependencies": {
    "@types/amqplib": "0.8.2",
    "@types/body-parser": "1.19.2",
    "@types/express": "4.17.13",
    "@types/jsonpath": "0.2.0",
    "@types/multer": "1.4.7",
    "@types/node": "17.0.31",
    "@types/supertest": "2.0.12",
    "@types/swagger-ui-express": "4.1.3",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "c8": "7.11.3",
    "concurrently": "7.1.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "nock": "13.2.4",
    "nodemon": "2.0.16",
    "prettier": "2.6.2",
    "prisma": "3.14.0",
    "supertest": "6.2.3",
    "ts-node": "10.7.0",
    "tsc-alias": "1.6.7",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.6.4",
    "vitest": "0.12.6",
    "vitest-mock-extended": "0.1.3"
  },
  "scripts": {
    "build": "npm run tsoa && tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "format": "eslint src/**/*.ts --fix",
    "lint": "eslint src/**/*.ts",
    "prisma:generate": "npx prisma generate",
    "tsoa": "tsoa spec-and-routes",
    "start": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "serve": "node ./dist/src/main.js",
    "setup:redis": "docker run -d --name redis-stack -p 6379:6379 -p 8001:8001 redis/redis-stack:latest",
    "start-brew:redis": "brew services start redis",
    "stop-brew:redis": "brew services stop redis",
    "start:rabbit": "docker run -d -it --rm --name rabbitmq -p 5672:5672 -p 15672:15672 rabbitmq:3.10-management",
    "stop:rabbit": "docker stop rabbitmq",
    "start-brew:rabbit": "brew services start rabbitmq",
    "stop-brew:rabbit": "brew services stop rabbitmq",
    "test": "vitest run --config vitest.config.ts",
    "test:watch": "vitest --config vitest.config.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BA-LouisAndrew/poc.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BA-LouisAndrew/poc/issues"
  },
  "homepage": "https://github.com/BA-LouisAndrew/poc#readme"
}
