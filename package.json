{
  "name": "poc",
  "version": "1.0.0",
  "description": "Proof of concepts",
  "main": "index.js",
  "dependencies": {
    "body-parser": "1.20.0",
    "cors": "2.8.5",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "express": "4.18.1",
    "redis": "4.1.0",
    "swagger-ui-express": "4.4.0",
    "tsoa": "4.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/express": "4.17.13",
    "@types/multer": "1.4.7",
    "@types/node": "17.0.31",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "concurrently": "7.1.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "nodemon": "2.0.16",
    "prettier": "2.6.2",
    "ts-node": "10.7.0",
    "tsc-alias": "1.6.7",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.6.4",
    "vitest": "0.12.6"
  },
  "scripts": {
    "build": "npm run tsoa && tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "format": "eslint src/**/*.ts --fix",
    "lint": "eslint src/**/*.ts",
    "tsoa": "tsoa spec-and-routes",
    "start": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "serve": "node ./dist/src/main.js",
    "setup:redis": "docker run -p 6379:6379 --name redis-redisjson redislabs/rejson:latest",
    "start:redis": "docker start redis-redisjson",
    "stop:redis": "docker stop redis-redisjson",
    "test": "vitest run --config vitest.config.ts",
    "test:watch": "vitest --config vitest.config.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BA-LouisAndrew/poc.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BA-LouisAndrew/poc/issues"
  },
  "homepage": "https://github.com/BA-LouisAndrew/poc#readme"
}