{
 "entryFile": "src/app.ts",
 "noImplicitAdditionalProperties": "throw-on-extras",
 "controllerPathGlobs": [
  "src/**/*Controller.ts"
 ],
 "spec": {
  "outputDirectory": "src/tsoa",
  "specVersion": 3
 },
 "routes": {
  "routesDir": "src/tsoa"
 },
 "compilerOptions": {
  "baseUrl": ".",
  "paths": {
   "@/*": [
    "./src/*"
   ],
   "tsoa": [
    "node_modules/tsoa/dist"
   ],
   "tsoa/": [
    "node_modules/tsoa/dist/"
   ]
  }
 }
}